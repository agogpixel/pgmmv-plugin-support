{
  "name": "@agogpixel/pgmmv-plugin-support",
  "description": "Implementation support for Pixel Game Maker MV plugins.",
  "license": "UNLICENSED",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/agogpixel/pgmmv-plugin-support"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "postinstall": "husky install",
    "prebuild": "cavy clean dist",
    "build": "tsc",
    "postbuild": "cavy copy dist && shx cp LICENSE README.md dist",
    "build-docs": "typedoc --out dist/docs --entryPointStrategy expand src/index.ts && shx cp LICENSE dist/docs",
    "create-tarball": "cavy pack dist",
    "publish-tarball": "cavy publish dist",
    "lint": "cavy lint --all",
    "lint-fix": "cavy lint --all --fix",
    "clean": "cavy clean --all"
  },
  "dependencies": {
    "@agogpixel/pgmmv-ts": "0.0.0--dev-build-1646116799"
  },
  "devDependencies": {
    "@agogpixel/cavy": "~0.1.0",
    "@typescript-eslint/eslint-plugin": "~5.13.0",
    "@typescript-eslint/parser": "~5.13.0",
    "eslint": "~8.10.0",
    "eslint-config-prettier": "~8.4.0",
    "eslint-plugin-prettier": "~4.0.0",
    "husky": "~7.0.4",
    "lint-staged": "~12.3.4",
    "prettier": "~2.5.1",
    "shx": "~0.3.4",
    "typedoc": "~0.22.12",
    "typescript": "~4.5.5"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --cache --fix",
    "*.(json|md|yml)": "prettier --write"
  }
}
